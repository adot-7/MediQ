{
    "builds": [
      {
        "src": "hosman/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "(.*)",
        "dest": "hosman/wsgi.py"
      }
    ],
    "hooks": {
    "build": "python manage.py collectstatic --noinput"
    }
  }
  